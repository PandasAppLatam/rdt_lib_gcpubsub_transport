{
  "name": "@flosportsinc/nestjs-google-pubsub-microservice",
  "version": "0.0.0-development",
  "description": "A NestJS Custom Transport Strategy for Google PubSub",
  "main": "dist/index.js",
  "repository": {
    "url": "git+https://github.com/flocasts/nestjs-google-pubsub-microservice.git",
    "type": "git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage --ci",
    "test:debug": "jest --max-workers=1",
    "build": "tsc",
    "prepare": "husky install && npm run build",
    "format": "prettier \"lib/**/*.ts\" \"examples/**/*.ts\"  -c",
    "format:fix": "npm run format -- --write",
    "semantic-release": "npx --no-install semantic-release"
  },
  "keywords": [
    "nestjs",
    "google",
    "pubsub"
  ],
  "author": "Harold Waters <harold.waters@flosports.tv>",
  "contributors": [
    {
      "name": "William Reynolds",
      "email": "william.reynolds@flosports.tv"
    }
  ],
  "license": "ISC",
  "release": {
    "branches": ["master", "next"]
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/microservices": "^7.6.15",
    "@types/jest": "^26.0.22",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.23.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.7",
    "semantic-release": "^17.4.2",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^2.10.0"
  },
  "peerDependencies": {
    "@nestjs/common": ">=7.0 && <8",
    "@nestjs/core": ">=7.0 && <8",
    "@nestjs/microservices": ">=7.0 && <8",
    "reflect-metadata": "^0.1.13",
    "rxjs": ">=6 && <7"
  },
  "lint-staged": {
    "{lib,test,examples}/**/*.ts": "prettier --write",
    "{lib,apps,examples,libs,test}/**/*.ts": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run build && npm run test"
    }
  }
}
